{
    "name": "performance",
    "version": "1.0.0",
    "description": "",
    "author": "hunjuly@gmail.com",
    "main": "index.js",
    "scripts": {
        "start": "node build/dist",
        "watch": "npm-run-all -p watch:*",
        "watch:bundle": "webpack --watch --mode development",
        "watch:nodemon": "nodemon --inspect build/dist",
        "build": "npm-run-all -s build:*",
        "build:clean": "rm -rf build",
        "build:prettier": "prettier -c --write src/**",
        "build:eslint": "eslint src/**",
        "build:compile": "tsc --noEmit true",
        "build:jest": "jest --no-cache --coverage --detectOpenHandles",
        "build:bundle": "webpack --mode production",
        "profile": "npm-run-all -s profile:*",
        "profile:clean": "rm -rf build",
        "profile:bundle": "webpack --mode development",
        "profile:start": "node --prof --no-logfile-per-isolate build/dist"
    },
    "dependencies": {
        "@babel/runtime": "^7.17.2",
        "common": "file:../common.node",
        "dotenv": "^16.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.17.5",
        "@babel/plugin-proposal-class-properties": "^7.16.7",
        "@babel/plugin-proposal-decorators": "^7.17.2",
        "@babel/plugin-transform-runtime": "^7.17.0",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-typescript": "^7.16.7",
        "@types/jest": "^27.4.1",
        "@types/morgan": "^1.9.3",
        "@types/node": "^17.0.21",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "babel-jest": "^27.5.1",
        "babel-loader": "^8.2.3",
        "babel-plugin-module-resolver": "^4.1.0",
        "clean-webpack-plugin": "^4.0.0",
        "eslint": "^8.9.0",
        "eslint-config-prettier": "^8.4.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.0.0",
        "nock": "^13.2.4",
        "typescript": "^4.5.5",
        "webpack": "^5.69.1",
        "webpack-cli": "^4.9.2",
        "webpack-node-externals": "^3.0.0"
    }
}
